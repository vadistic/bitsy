{
  "name": "bitsy-nosql-bucket",
  "version": "0.1.0",
  "description": "Simple data store API for backendless prototyping",
  "author": "Jakub Wadas<vadistic@gmail.com>",
  "private": true,
  "license": "MIT",
  "keywords": [
    "rest",
    "api",
    "nosql",
    "mongo",
    "nestjs"
  ],
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prestart:prod": "yarn build",
    "postinstall": "yarn prestart:prod",
    "deploy": "heroku git:remote -a bitsy-nosql-bucket"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.8",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.8",
    "@nestjs/platform-express": "^7.0.8",
    "@nestjs/schedule": "^0.3.1",
    "@nestjs/serve-static": "^2.1.0",
    "@nestjs/swagger": "^4.5.3",
    "cache-manager": "^3.2.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.1",
    "convict": "^6.2.2",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "mongodb": "^3.5.6",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "swagger-ui-express": "^4.1.4",
    "tslib": "^1.11.1",
    "unique-names-generator": "^4.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.8",
    "@types/convict": "^4.2.1",
    "@types/cron": "^1.7.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "25.2.1",
    "@types/mongodb": "^3.5.8",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.13.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.4.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "../test/setup-test.ts"
    ]
  }
}
